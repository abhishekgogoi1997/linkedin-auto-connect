{
    "manifest_version": 3,
    "name": "LinkedIn Auto Connect",
    "version": "1.0",
    "description": "Automatically presses 'Connect' buttons on LinkedIn.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "share.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*.linkedin.com/*"],
        "js": ["content.js"]
      }
    ]
  }